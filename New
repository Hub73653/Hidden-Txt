                    Teleport(findplayer(args[2]),Trap)
                end
            end
            if cmd == Prefix.."loopkill" or cmd == Prefix.."lk" then
                if args[2]:lower() == "all" then
                    _G.LoopKill = true
                    while _G.LoopKill == true do
                        wait()

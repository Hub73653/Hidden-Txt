        end
        if rankedcmds == Prefix.."gate-tower" then
            Teleport(Player,GateTower)
        end
        if rankedcmds == Prefix.."sewer" then
            Teleport(Player,Sewer)
        end
    end

    for i,v in pairs(game.Players:GetPlayers()) do
        if v ~= game.Players.LocalPlayer then
            CheckPermissions(v)
        end
    end


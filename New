-- // Game Checking

if game.PlaceId == Games.PrisonLife then

    -- // Customizable

    local Void = CFrame.new(999999, 999999, 999999)
    local Trap = CFrame.new(-346.800995, 84.2401199, 1970.80005, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    local Base = CFrame.new(-943.283997, 94.1287613, 2057.86011, 1, 7.36142525e-09, -1.57051008e-13, -7.36142525e-09, 1, -1.28909488e-08, 1.56956113e-13, 1.28909488e-08, 1)
    local Prison = CFrame.new(906.399658, 100.638306, 2388.50073, -1, 0, 0, 0, 1, 0, 0, 0, -1)
    local Yard = CFrame.new(782.20697, 97.9999008, 2468.15991, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    local Armory = CFrame.new(836.43811, 99.9899826, 2284.13965, -0.999900043, 3.05707992e-08, 0.0141399782, 2.92557178e-08, 1, -9.32113906e-08, -0.0141399782, -9.27883974e-08, -0.999900043)
    local Hell = CFrame.new(-584.465149, 10.8399096, 1434.67139, 0.0703570694, -2.91384534e-08, 0.997521877, -2.77830541e-08, 1, 3.11704333e-08, -0.997521877, -2.99072624e-08, 0.0703570694)

    local Nexus = CFrame.new(888, 100, 2388)
    local Cafe = CFrame.new(888, 100, 2388)
    local BackNexus = CFrame.new(982, 100, 2334)
    local Gate = CFrame.new(505, 103, 2250)
    local Tower = CFrame.new(822, 131, 2588)
    local GateTower = CFrame.new(502, 126, 2306)
    local Sewer = CFrame.new(916, 79, 2311)

    -- // Other

    local function GetTeam()
        return game.Players.LocalPlayer.TeamColor.Name
    end

    -- // Configure

    Admin = {}

    -- // Notifying

    notif("VERSION","You're currently running Sin Version: "..Vers.."!")

    -- // Toggles

    _G.LoopKill = false
    _G.LoopBring = false
    _G.LoopTo = false
    _G.AutoGuns = false
    _G.SpamArrest = false
    _G.SpamVoid = false

    -- // Defining

    local Backpack = LP.Backpack
    Clipon = false

    -- // Grabbing the Tag

    LP.Character:BreakJoints() -- Kills.

    -- // General Importance

    local Workspace = game:GetService("Workspace")

    -- // Team Changer

    local savedcf = GetPos()
    local savedcamcf = GetCamPos()
    workspace.Remote.loadchar:InvokeServer(nil, BrickColor.new("White").Name)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = savedcf
    workspace.CurrentCamera.CFrame = savedcamcf

    -- // Cmd Functions

    function Teleport(Player, Position)
        if Player == nil or Position == nil then return end
        local savedcf = GetPos()
        workspace.Remote.loadchar:InvokeServer()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = savedcf
        workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.single["Crude Knife"].ITEMPICKUP)
        local CHAR = game.Players.LocalPlayer.Character
        CHAR.Humanoid.Name = "1"
        local c = CHAR["1"]:Clone()
        c.Name = "Humanoid"
        c.Parent = CHAR
        CHAR["1"]:Destroy()
        game.Workspace.CurrentCamera.CameraSubject = CHAR
        CHAR.Animate.Disabled = true
        wait()
        CHAR.Animate.Disabled = false
        CHAR.Humanoid.DisplayDistanceType = "None"
        game.Players.LocalPlayer:FindFirstChild("Backpack"):FindFirstChild("Crude Knife").Parent = CHAR
        local STOP = 0
        repeat wait(.3)
            STOP = STOP + 1
            Player.Character.HumanoidRootPart.CFrame = LP.Character.HumanoidRootPart.CFrame
        until (not game.Players.LocalPlayer.Character:FindFirstChild("Crude Knife") or not game.Players.LocalPlayer.Character.HumanoidRootPart or not Player.Character.HumanoidRootPart or not game.Players.LocalPlayer.Character.HumanoidRootPart.Parent or not Player.Character.HumanoidRootPart.Parent or STOP > 500) and STOP > 3
        local STOP_2 = 0
        repeat wait()
            STOP_2 = STOP_2 + 1
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Position
        until STOP_2 > 10
        workspace.Remote.loadchar:InvokeServer()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = savedcf
    end

    function ArrestEvent(Player, Time)
        for i = 1,Time do
            workspace.Remote.arrest:InvokeServer(Player.Character.Head)
        end
    end
    
    function Arrest(Player, Time)
        local Time = Time or 1
        local savedcf = GetPos()
        local savedcamcf = GetCamPos()
        local savedteam = GetTeam()
        if Player then
            repeat wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1)
                for i = 1,Time do
                    coroutine.wrap(function()
                        workspace.Remote.arrest:InvokeServer(Player.Character.Head)
                    end)()
                end
            until Player.Character:FindFirstChild("Head"):FindFirstChild("handcuffedGui")
            wait()
        end
        workspace.Remote.loadchar:InvokeServer(nil, BrickColor.new(savedteam).Name)
        game.Players.LocalPlayer.Character.Humanoid.Sit = false
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = savedcf
        workspace.CurrentCamera.CFrame = savedcamcf
    end

    function Kill(Player)
        pcall(function()
            workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver["Remington 870"].ITEMPICKUP)
            local Gun = game.Players.LocalPlayer.Backpack:FindFirstChild("Remington 870") or game.Players.LocalPlayer.Character:FindFirstChild("Remington 870")
            local FireEvent = {
                [1] = {
                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                    ["Distance"] = 0, 
                    ["Cframe"] = CFrame.new(), 
                    ["Hit"] = workspace[Player.Name].Head
                }, [2] = {
                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                    ["Distance"] = 0, 
                    ["Cframe"] = CFrame.new(), 
                    ["Hit"] = workspace[Player.Name].Head
                }, [3] = {
                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                    ["Distance"] = 0, 
                    ["Cframe"] = CFrame.new(), 
                    ["Hit"] = workspace[Player.Name].Head
                }, [4] = {
                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                    ["Distance"] = 0, 
                    ["Cframe"] = CFrame.new(), 
                    ["Hit"] = workspace[Player.Name].Head
                }, [5] = {
                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                    ["Distance"] = 0, 
                    ["Cframe"] = CFrame.new(), 
                    ["Hit"] = workspace[Player.Name].Head
                }
            }
            game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent,Gun)
            Gun.Parent = LP.Character
            game.Players.LocalPlayer.Character["Remington 870"].Parent = game.ServerStorage
        end)
    end

    function CheckPermissions(Player)
        Player.Chatted:Connect(function(Message)
            if Admin[Player.UserId] then
                AdminPlayerChatted(Message, Player)
            end
        end)
    end

    function AdminPlayerChatted(Message, Player)
        local args = Message:split(" ")
        local rankedcmds = args[1]
        local Aimed = findplayer(args[2])
        if rankedcmds == Prefix.."cmds" or rankedcmds == Prefix.."commands" then
            talk("Prefix is "..Prefix.." | Commands: base, yard, void [plr], prison, armory, kill [all, team, plr], arrest [all, plr], to / goto [plr], bring [plr], nexus, cafe, back-nexus, gate, tower, gate-tower, sewer")
        end
        if rankedcmds == Prefix.."base" then
            Teleport(Player,Base)
        end
        if rankedcmds == Prefix.."yard" then
            Teleport(Player,Yard)
        end
        if rankedcmds == Prefix.."void" then
            local targ = findplayer(args[2])
            Teleport(targ,Void)
        end
        if rankedcmds == Prefix.."prison" then
            Teleport(Player,Prison)
        end
        if rankedcmds == Prefix.."armory" then
            Teleport(Player,Armory)
        end
        if rankedcmds == Prefix.."kill" then
            if args[2]:lower() == "all" then
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        Kill(v)
                    end
                end
            elseif args[2]:lower() == "others" then
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        Kill(v)
                    end
                end
            elseif args[2]:lower() == "inmates" or args[2]:lower() == "prisoners" then
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        if v.TeamColor.Name == "Bright orange" then
                            Kill(v)
                        end
                    end
                end
            elseif args[2]:lower() == "cops" or args[2]:lower() == "police" or args[2]:lower() == "guards" then
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        if v.TeamColor.Name == "Bright blue" then
                            Kill(v)
                        end
                    end
                end
            elseif args[2]:lower() == "crims" or args[2]:lower() == "criminals" then
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        if v.TeamColor.Name == "Really red" then
                            Kill(v)
                        end
                    end
                end
            else
                Kill(findplayer(args[2]))
            end
        end
        if rankedcmds == Prefix.."arrest" then
            if args[2]:lower() == "all" or args[2]:lower() == "others" then
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer then
                        if v.TeamColor.Name == "Really red" then
                            Arrest(v, 30)
                        end
                    end
                end
            else
                Arrest(findplayer(args[2]))
            end
        end
        if rankedcmds == Prefix.."to" or rankedcmds == Prefix.."goto" then
            local targ = findplayer(args[2])
            Teleport(Player,targ.Character.HumanoidRootPart.CFrame)
        end
        if rankedcmds == Prefix.."bring" then
            Teleport(findplayer(args[2]),Player.Character.HumanoidRootPart.CFrame)
        end
        if rankedcmds == Prefix.."nexus" then
            Teleport(Player,Nexus)
        end
        if rankedcmds == Prefix.."cafe" then
            Teleport(Player,Cafe)

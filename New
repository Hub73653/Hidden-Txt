            end
            coroutine.wrap(YYHNVQ_fake_script)()

            Commands.Back.Visible = true
            Commands.Area.Visible = true
            Commands.Back:TweenPosition(UDim2.new(0.5,0,0.825,0),"InOut","Linear",.5)
            Commands.Area:TweenPosition(UDim2.new(0.5,0,0.825,0),"InOut","Linear",.5)
        end
        if cmd == Prefix.."hidecmds" then
            local Commands = LP.PlayerGui:FindFirstChild("Commands")
            Commands.Back:TweenPosition(UDim2.new(0.5,0,2,0),"InOut","Linear",.5)
            Commands.Area:TweenPosition(UDim2.new(0.5,0,2,0),"InOut","Linear",.5)
            wait(.51)
            Commands:Destroy()
        end
        if cmd == Prefix.."rejoin" or cmd == Prefix.."rej" or cmd == Prefix.."rj" then
            local TeleportService = game:GetService("TeleportService")
            local Players = game:GetService("Players")
            local LocalPlayer = Players.LocalPlayer
            local Rejoin = coroutine.create(function()
                local Success, ErrorMessage = pcall(function()
                    TeleportService:Teleport(game.PlaceId, LocalPlayer)
                end)
                if ErrorMessage and not Success then
                    warn(ErrorMessage)
                end
            end)
            coroutine.resume(Rejoin)
        end
        if cmd == Prefix.."serverhop" or cmd == Prefix.."sh" or cmd == Prefix.."hop" or cmd == Prefix.."shop" then
            local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100"))
            for i,v in pairs(Servers.data) do
                if v.playing ~= v.maxPlayers then
                    game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
                end
            end
        end
        if cmd == Prefix.."devs" then
            notif("Owner, ","Lead Developer",Owner,1)
            notif("Co-Owner, ","Side Developer",CoOwner,1)
        end
        if cmd == Prefix.."noclip" then
            local Stepped = game:GetService("RunService")
            Clipon = true
            Stepped.Stepped:Connect(function()
                if not Clipon == false then
                    for a, b in pairs(Workspace:GetChildren()) do
                    if b.Name == LP.Name then
                    for i, v in pairs(Workspace[LP.Name]:GetChildren()) do
                    if v:IsA("BasePart") then
                    v.CanCollide = false
                    end end end end
                else
                    Stepped:Disconnect()
                end
            end)
        end
        if cmd == Prefix.."clip" then
            Clipon = false
        end
        if cmd == Prefix.."leave" or cmd == Prefix.."quit" then
            LP:Kick("You have left the game!")

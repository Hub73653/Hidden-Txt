function talk(words)
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(words,"all")
end

function notif(title,desc)
    local COPY_DELETE_AFTER_ANIM = Instance.new("ScreenGui")
    local messageGui = Instance.new("Frame")
    local ImageLabel = Instance.new("ImageLabel")
    local Title = Instance.new("TextLabel")
    local Desc = Instance.new("TextLabel")
    COPY_DELETE_AFTER_ANIM.Name = "COPY_DELETE_AFTER_ANIM"
    COPY_DELETE_AFTER_ANIM.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    COPY_DELETE_AFTER_ANIM.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    messageGui.Name = "messageGui"
    messageGui.Parent = COPY_DELETE_AFTER_ANIM
    messageGui.AnchorPoint = Vector2.new(0.5, 0.5)
    messageGui.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    messageGui.BackgroundTransparency = 0.400
    messageGui.BorderColor3 = Color3.fromRGB(53, 53, 53)
    messageGui.Position = UDim2.new(0.5, 0, 0.248999998, 0)
    messageGui.Size = UDim2.new(0, 1920, 0, 94)
    ImageLabel.Parent = messageGui
    ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ImageLabel.BackgroundTransparency = 1.000
    ImageLabel.Size = UDim2.new(0, 1920, 0, 94)
    ImageLabel.Image = "rbxassetid://267232285"
    ImageLabel.ImageTransparency = 0.500
    Title.Name = "Title"
    Title.Parent = messageGui
    Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Title.BackgroundTransparency = 1.000
    Title.Size = UDim2.new(0, 1920, 0, 51)
    Title.Font = Enum.Font.SourceSansBold
    Title.Text = "PRISON ANNOUNCEMENT"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextScaled = true
    Title.TextSize = 14.000
    Title.TextStrokeTransparency = 0.000
    Title.TextWrapped = true
    Desc.Name = "Desc"
    Desc.Parent = messageGui
    Desc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Desc.BackgroundTransparency = 1.000
    Desc.Position = UDim2.new(0, 0, 0.5, 0)
    Desc.Size = UDim2.new(0, 1920, 0, 47)
    Desc.Font = Enum.Font.SourceSans
    Desc.Text = "All prisoners must report to the cafeteria for dinner."
    Desc.TextColor3 = Color3.fromRGB(255, 255, 255)
    Desc.TextScaled = true
    Desc.TextSize = 14.000
    Desc.TextWrapped = true

    local function AnimationScript()
        local script = Instance.new('LocalScript', COPY_DELETE_AFTER_ANIM)

        -- Script made by KingSuno#9767
        
        -- Variables
        
        local messageGui = script.Parent.messageGui
        local Desc = messageGui.Desc
        local Title = Desc.Parent.Title
        
        -- Defaults
        
        messageGui.Position = UDim2.new(0.5,0,-0.5,0)
        
        -- Text Changing
        
        Title.Text = title
        Desc.Text = desc
        
        -- Services
        
        local ts = game:GetService("TweenService")
        local ti = TweenInfo.new(1.75,Enum.EasingStyle.Circular,Enum.EasingDirection.InOut)
        
        -- Animating
        
        local Tween = ts:Create(messageGui,ti,{Position = UDim2.new(0.5, 0,0.249, 0)})
        Tween:Play()
        
        wait(4.5)
        
        local OutTween = ts:Create(messageGui,ti,{Position = UDim2.new(0.5, 0,-0.5, 0)})
        OutTween:Play()

        wait(1)

        COPY_DELETE_AFTER_ANIM:Destroy() -- Removes the Gui
        
        -- End of script (:
        
    end
    coroutine.wrap(AnimationScript)()
end

function othernotif(title,desc,img,time)
    game.StarterGui:SetCore("SendNotification",{
        Title = title;
        Text = desc;
        Icon = img;
        Duration = time;
    })
end

-- // Functions

function findplayer(String)
    if not String then return end
    local Yes = {}
    for _, Player in ipairs(game.Players:GetPlayers()) do
        if string.lower(Player.Name):match(string.lower(String)) or string.lower(Player.DisplayName):match(string.lower(String)) then
            table.insert(Yes, Player)
        end
    end
    if #Yes > 0 then
        return Yes[1]
    elseif #Yes < 1 then
        return nil
    end
end

function Load(https)
    loadstring(game:HttpGetAsync(https))()
end

function chatnotify(text,rgb)
    game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
        Text = (text);
        Color = rgb;
        Font = Enum.Font.SourceSansBold; 
        FontSize = Enum.FontSize.Size18;
    })
end


if game.PlaceId == Games.R6Baseplate then

    -- // Notifying

    notif("Ranked","Say "..Prefix.."cmds for cmds",Valid,1)
    wait(1)
    notif("Version","On Version: "..Vers.."!",Valid,1)

    -- // Cmds

    LP.Chatted:Connect(function(msg)
        local args = msg:split(" ")
        local cmd = args[1]
        if cmd == Prefix.."cmds" or cmd == Prefix.."commands" then
            local Commands = Instance.new("ScreenGui")
            local Area = Instance.new("Frame")
            local UICorner = Instance.new("UICorner")
            local Back = Instance.new("Frame")
            local UICorner_2 = Instance.new("UICorner")
            local Items = Instance.new("ScrollingFrame")
            local UIListLayout = Instance.new("UIListLayout")
            Commands.Name = "Commands"
            Commands.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
            Commands.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

            Area.Name = "Area"
            Area.Parent = Commands
            Area.AnchorPoint = Vector2.new(0.5, 0.5)
            Area.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            Area.BackgroundTransparency = 0.800
            Area.Position = UDim2.new(0.5, 0, 2, 0)
            Area.Size = UDim2.new(0, 331, 0, 331)

            UICorner.CornerRadius = UDim.new(0, 3)
            UICorner.Parent = Area

            Back.Name = "Back"
            Back.Parent = Commands
            Back.AnchorPoint = Vector2.new(0.5, 0.5)
            Back.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            Back.Position = UDim2.new(0.5, 0, 2, 0)
            Back.Size = UDim2.new(0, 325, 0, 325)

            UICorner_2.CornerRadius = UDim.new(0, 3)
            UICorner_2.Parent = Back

            Items.Name = "Items"
            Items.Parent = Back
            Items.Active = true
            Items.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Items.BackgroundTransparency = 1.000
            Items.Position = UDim2.new(0.0276923068, 0, 0.0338461548, 0)
            Items.Size = UDim2.new(0, 307, 0, 302)
            Items.CanvasSize = UDim2.new(0, 0, 4.37, 0)
            Items.ScrollBarThickness = 0

            UIListLayout.Parent = Items
            UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
            UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            local function YYHNVQ_fake_script() -- Commands.Cmd 
                local script = Instance.new('LocalScript', Commands)

                local c = {}
                
                function c(text)
                    local Command = Instance.new("TextLabel")
                    Command.Name = "Command"
                    Command.Parent = game.Players.LocalPlayer.PlayerGui.Commands.Back.Items
                    Command.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    Command.BackgroundTransparency = 1.000
                    Command.Position = UDim2.new(0.174267098, 0, 0, 0)
                    Command.Size = UDim2.new(0, 200, 0, 29)
                    Command.Font = Enum.Font.GothamSemibold
                    Command.Text = text
                    Command.TextColor3 = Color3.fromRGB(255, 255, 255)
                    Command.TextSize = 14.000
                end
                
                c("cmds / commands")
                c("hidecmds")
                c("rejoin / rj / rej")
                c("serverhop / hop / sh")
                c("devs")
                c("noclip [other: clip]")

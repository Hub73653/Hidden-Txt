local function LMQBF_fake_script() -- LoadUi.Animations 
	local script = Instance.new('LocalScript', LoadUi)

	local ts = game:GetService("TweenService")
	local ti = TweenInfo.new(0.5,Enum.EasingStyle.Cubic)
	
	wait(2)
	
	local YourProf = script.Parent.YourProfile
	
	local Y_Fade = ts:Create(YourProf,ti,{ImageTransparency = 1})
	local Y2_Fade = ts:Create(YourProf,ti,{BackgroundTransparency = 1})
	Y2_Fade:Play()
	Y_Fade:Play()
	local Y_Size = ts:Create(YourProf,ti,{Size = UDim2.new(0,10000,0,10000)})
	Y_Size:Play()
end
coroutine.wrap(LMQBF_fake_script)()

